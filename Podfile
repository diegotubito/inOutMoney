# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'DebugVersion' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for DebugVersion
pod 'Firebase'
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  pod 'Firebase/RemoteConfig'

  pod 'ReachabilitySwift'
  pod 'Alamofire'
  pod 'Charts'

end

target 'ReleaseVersion' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ReleaseVersion
pod 'Firebase'
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  pod 'Firebase/RemoteConfig'

  pod 'ReachabilitySwift'
  pod 'Alamofire'
  pod 'Charts'

end
